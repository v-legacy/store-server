<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>#No</th>
      <th>Coin Name</th>
      <th>Coin Quantity</th>
      <th>Coin Price</th>
      <th>Option</th>

    </tr>
  </thead>
  <tbody>
    <% for( let index = 0; index < data.length; index++ ) { %>
    <tr>
      <td><%= index + 1 %></td>
      <td><%= data[index].coinName %></td>
      <td><%= data[index].coinQuantity %></td>
      <td><%= data[index].price %></td>
      <td>
        <form action="/nominal/delete/<%= data[index]._id %>?_method=DELETE" method="POST">
          <a href="/nominal/edit/<%= data[index]._id %>" class="btn btn-warning btn-sm"><i class="fas fa-pencil"></i> Edit</a>
          <button class="btn btn-danger btn-sm"><i class="fas fa-trash"></i> Delete</button>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
